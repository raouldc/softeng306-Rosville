define robfield position
(
  #Shape of robot
  block
  (
    points 4
    point[0] [0 0]
    point[1] [1 0]
    point[2] [1 1]
    point[3] [0 1]
    z [0 1]
  )
  #fit shape to this size in meters
  size [15 15 0.25]
  drive "diff"
  mylaser(pose [ 0.050 1.000 0 0.000 ])
)


define floorplan model
(
  # sombre, sensible, artistic
  color "gray30"

  # most maps will need a bounding box
  boundary 1
  
  ranger_return 1.000
)

define my_block model
(
  size [0.5 0.5 0.5]
  gui_nose 0
)


